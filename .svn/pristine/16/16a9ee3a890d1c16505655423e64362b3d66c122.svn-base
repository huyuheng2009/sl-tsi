	<build>
		<finalName>couriertsi</finalName>
		<pluginManagement>
			<plugins>
				<!-- compiler鎻掍欢, 璁惧畾JDK鐗堟湰 -->
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.1</version>
					<configuration>
						<source>1.6</source>
						<target>1.6</target>
						<encoding>UTF-8</encoding>
						<optimize>true</optimize>
						<compilerId>eclipse</compilerId>
					</configuration>
					<dependencies>
						<!-- <dependency> <groupId>org.codehaus.plexus</groupId> <artifactId>plexus-utils</artifactId> 
						<version>3.0.15</version> </dependency> -->
						<dependency>
							<groupId>org.codehaus.plexus</groupId>
							<artifactId>plexus-compiler-eclipse</artifactId>
							<version>2.2</version>
						</dependency>
					</dependencies>
				</plugin>
				<!-- tomcat plugin -->
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>tomcat-maven-plugin</artifactId>
					<version>1.1</version>
					<configuration>
						<path>/</path>
						<port>6066</port>
						<charset>utf-8</charset>
						<uriEncoding>UTF-8</uriEncoding>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.mortbay.jetty</groupId>
					<artifactId>jetty-maven-plugin</artifactId>
					<version>7.6.0.RC5</version>
					<configuration>
						<systemProperties>
							<systemProperty>
								<name>debug</name>
								<value>true</value>
							</systemProperty>
							<systemProperty>
								<name>log4j.configuration</name>
								<value>log4j_dev.properties</value>
							</systemProperty>
						</systemProperties>
						<connectors>
							<connector implementation="org.eclipse.jetty.server.bio.SocketConnector">
								<port>6066</port>
							</connector>
						</connectors>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
	<properties>
