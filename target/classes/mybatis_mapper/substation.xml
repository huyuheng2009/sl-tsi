<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yogapay.couriertsi.services2.SubstationService2">

	<select id="getByNO" parameterType="string">  
		SELECT NULL AS ':ENTITY(Substation)', s1.* FROM substation s1 WHERE s1.substation_no=#{value}
	</select>

</mapper>